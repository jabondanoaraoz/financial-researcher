# API Keys
GROQ_API_KEY=your_groq_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
FRED_API_KEY=your_fred_api_key_here
ANTHROPIC_API_KEY=your_claude_api_key_here  # Optional: for Claude API
DEEPSEEK_API_KEY=your_deepseek_key_here     # Optional: for DeepSeek API

# LLM Configuration
LLM_PROVIDER=groq  # Options: groq, claude, ollama, deepseek
LLM_MODEL=llama-3.3-70b-versatile  # Model name (depends on provider)
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=8000

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL_HOURS=24
CACHE_DB_PATH=./cache/financial_data.db

# Data Settings
DEFAULT_CURRENCY=USD
PREFERRED_EXCHANGE=NYSE  # NYSE, NASDAQ, etc.

# Output Settings
OUTPUT_DIR=./output
EXCEL_TEMPLATE_MODE=ib_grade  # ib_grade, simple, detailed
